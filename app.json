{
  "expo": {
    "name": "pooki",
    "slug": "the-pooki-app",
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/images/naazi.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/naazi.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "sourav"
    },
    "android": {
      "package": "sourav",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/naazi.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/naazi.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/nothing.ttf"]
        }
      ],
      "expo-router",
      [
        "expo-font",
        {
          "custom": {
            "CambayBold": "./node_modules/@expo-google-fonts/cambay/Cambay_700Bold.ttf"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "e35d11dd-6eb3-4fb8-89cc-5b4781fa92ba"
      }
    }
  }
}
